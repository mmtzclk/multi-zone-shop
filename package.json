{
  "name": "multi-zone-shop",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "start": "turbo run start",
    "build:home": "turbo run build --filter=@apps/home",
    "build:cart": "turbo run build --filter=@apps/cart"
  },
  "devDependencies": {
    "turbo": "2.1.3",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "react-icons": "^5.5.0",
    "sonner": "^2.0.7"
  }
}
